  <div class=\"indicator\">\n      <div v-for=\"(item, index) in itens\" :key=\"item\">\n          <v-icon small :style=\"style(index)\">$iconCircle</v-icon>\n      </div>\n  </div>\n</template>\n\n<script>\nexport default {\n    name:\"indicator\",\n    props:{\n        have: Number,\n        of: Number,\n    },\n    data: function(){\n        return {\n            colorFirst: \"#000000\",\n            colorLast: \"#C4C4C4\",\n        }\n    },\n    computed:{\n        //with the \"have\" quantity is created\n        //a list to be used in \"v-for\"\n        itens: function(){\n            var index = this.have;\n            var list = [];\n            while(index > 0){\n                list.push(index);\n                index --;\n            }\n\n            return list;\n        }\n    },\n    methods:{\n        style: function(index){\n            //calcula cor\n            var r =parseInt(this.colorFirst.slice(1, 3), 16);\n            var rL = parseInt(this.colorLast.slice(1, 3), 16);\n            var rN = parseInt((((rL-r)/this.have)*index)+r).toString(16);\n            var style = \"color: #\"+rN+rN+rN+\"; margin-right: -.3em\";\n            \n            console.log(style);\n\n            return style;\n        }\n    }\n}\n</script>\n\n<style>\n.indicator{\n    display: flex;\n    flex-direction: row;\n    margin-right: .3em;\n}\n</style>"],"sourceRoot":"src/components/card"}]}